  "dbg_tool_specs": [
    {
      "name": "execute",
      "description": "Execute an arbitrary GDB/pwndbg command.\n\n    Args:\n        command: Raw GDB/pwndbg command to execute (e.g., \"info registers\", \"vmmap\").\n\n    Returns:\n        Dict containing the raw MI/console responses, a success flag, and the current GDB state.\n    ",
      "inputSchema": {
        "properties": {
          "command": {
            "title": "Command",
            "type": "string"
          }
        },
        "required": [
          "command"
        ],
        "title": "executeArguments",
        "type": "object"
      },
      "outputSchema": {
        "properties": {
          "result": {
            "additionalProperties": true,
            "title": "Result",
            "type": "object"
          }
        },
        "required": [
          "result"
        ],
        "title": "executeOutput",
        "type": "object"
      }
    },
    {
      "name": "set_file",
      "description": "Load an executable file into GDB/pwndbg for debugging.\n\n    Args:\n        binary_path: Absolute path to the ELF to debug.\n\n    Returns:\n        Dict with MI command responses and state.\n    ",
      "inputSchema": {
        "properties": {
          "binary_path": {
            "title": "Binary Path",
            "type": "string"
          }
